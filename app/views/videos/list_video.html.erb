<ul class="list-group">
    <% @free_courses.each{ |course|%>
        <% course.videos.each{ |video|%>
            <li class="list-group-item list-group-item-info" style="line-height:30px;" >
                <%= link_to "#{course.course_description}: #{video.url}", videos_display_video_path(video_id: video.id) %>
            </li><br />
        <% } %>
    <% } %>
</ul>